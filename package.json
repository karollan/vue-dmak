{
	"name": "vue-dmak",
	"version": "1.0.3",
	"description": "Vue component wrapper for Draw Me A Kanji (dmak) - Render your Japanese writings with fun and taste",
	"main": "dist/vue-dmak.umd.js",
	"module": "dist/vue-dmak.es.js",
	"types": "index.d.ts",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"import": "./dist/vue-dmak.es.js",
			"require": "./dist/vue-dmak.umd.js"
		},
		"./style.css": "./dist/style.css",
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE",
		"index.d.ts"
	],
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"vue",
		"vue3",
		"vue-component",
		"kanji",
		"japanese",
		"draw",
		"svg",
		"animation",
		"dmak"
	],
	"author": "Karol Lange",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/karollan/vue-dmak.git"
	},
	"bugs": {
		"url": "https://github.com/karollan/vue-dmak/issues"
	},
	"homepage": "https://github.com/karollan/vue-dmak#readme",
	"peerDependencies": {
		"vue": "^3.0.0"
	},
	"dependencies": {
		"raphael": "^2.3.0"
	},
	"devDependencies": {
		"@vitejs/plugin-vue": "^4.5.0",
		"vite": "^5.0.0",
		"vue": "^3.3.0"
	}
}